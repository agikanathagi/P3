#include <iostream>
#include <String.h>

using namespace std;

int main()
{

    int Bulan;
    string bul;

    cout<<"Masukkan Bulan: ";
    cin>>Bulan;

    switch(Bulan){
        case 1:
            bul = "Januari";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 2:
            bul = "Februari";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 3:
            bul = "Maret";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 4:
            bul = "April";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 5:
            bul = "Mei";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 6:
            bul = "Juni";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 7:
            bul = "Juli";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 8:
            bul = "Agustus";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 9:
            bul = "September";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 10:
            bul = "Oktober";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 11:
            bul = "November";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        case 12:
            bul = "Desember";
            cout<<"Bulan ke - "<<Bulan<<" adalah "<<bul<<endl;
            break;

        default : cout<<"Tidak ada";
    }

    return 0;
}
